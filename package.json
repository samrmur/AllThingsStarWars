{
  "name": "AllThingsStarWars",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "postinstall": "patch-package",
    "start": "STORYBOOK=false node node_modules/react-native/local-cli/cli.js start --resetCache",
    "storybook": "((adb reverse tcp:7007 tcp:7007 || true) && start-storybook) & STORYBOOK=true node node_modules/react-native/local-cli/cli.js start --resetCache",
    "code-gen": "yarn download-schema && yarn convert-schema && yarn generate-types",
    "generate-types": "node scripts/generateTypes.js",
    "convert-schema": "node scripts/convertJsonSchema.js",
    "download-schema": "node scripts/downloadSchema.js",
    "clean": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-*",
    "check-style": "prettier --check \"src/**/*.{js,ts,tsx}\" \"storybook/**/*.{js,ts,tsx}\"",
    "fix-style": "prettier --write \"src/**/*.{js,ts,tsx}\" \"storybook/**/*.{js,ts,tsx}\"",
    "test": "jest",
    "check-typescript": "tsc --skipLibCheck",
    "lint": "eslint ./src --ext .js,.ts,.tsx --max-warnings 0 --format codeframe"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/material-bottom-tabs": "^5.2.10",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.6.2",
    "@shopify/react-i18n": "^6.2.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.12.5",
    "graphql-typed": "^1.0.0",
    "react": "16.11.0",
    "react-native": "0.62.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-paper": "^3.10.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.6",
    "react-native-screens": "^2.9.0",
    "react-native-v8": "^0.62.2-patch.1",
    "react-native-vector-icons": "^6.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/addon-ondevice-knobs": "^5.3.0",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.38",
    "@types/react-native": "^0.62.13",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "apollo-codegen": "^0.20.2",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.1",
    "graphql-typescript-definitions": "^0.18.1",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.0.5",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
